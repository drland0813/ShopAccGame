<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@100;200;300;400;500;600;700;800;900&display=swap"
          rel="stylesheet">
    <!-- font library -->
    <link rel="stylesheet" href="./assets/font/fontawesome-free-5.15.1-web/css/all.min.css">
    <!-- icon library -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/normalize/8.0.1/normalize.min.css">
    <!-- css reset -->
    <link rel="stylesheet" href="./assets/css/grid.css">
    <!-- grid library -->
    <link rel="stylesheet" href="./assets/css/base.css">
    <!-- base css -->
    <link rel="stylesheet" href="./assets/css/animation.css">
    <!-- animation library -->
    <link rel="stylesheet" href="./assets/css/main.css">
    <link rel="stylesheet" href="./assets/css/header.css">
    <!-- main css -->
    <link rel="stylesheet" href="./assets/css/reponsive.css">
    <!-- reponsive for device -->
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Sarpanch:wght@400;500;700&display=swap" rel="stylesheet">
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <input type="checkbox" name="rdo__menu" id="rdo__menu" hidden>
    <header class="header">
        <div class="header__logo">
            <a href="https://localhost:44333/" class="header__logo-link">
                <img class="logo" src="./assets/img/logo.png" alt="">
                <div class="logo__add hide-on-mobile">
                    <svg width="74" height="56" viewBox="0 0 74 56" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <path fill-rule="evenodd" clip-rule="evenodd"
                              d="M41.6623 56L44.6556 51.4544L70.2235 12.6282L73.5161 7.62817L67.5294 7.62817L63.7364 7.62817L65.4671 4.99999L68.7597 -1.4843e-05L62.773 -1.43196e-05L11.0387 -9.79684e-06L5.05195 -9.27346e-06L8.34456 4.99999L10.0753 7.62818L9.80839 7.62818L5.98675 7.62818L-4.2288e-06 7.62818L3.29261 12.6282L28.8605 51.4544L31.8539 56L34.8473 51.4544L36.7581 48.5527L38.6689 51.4544L41.6623 56ZM36.8915 39.2588L19.3546 12.6282L54.4284 12.6282L36.8915 39.2588ZM57.7496 7.62818L57.721 7.62818L16.062 7.62818L14.3313 4.99999L59.4803 4.99999L57.7496 7.62818ZM39.7658 44.0288L41.6623 46.9088L64.2368 12.6282L60.4438 12.6282L39.8992 43.8262L39.7658 44.0288ZM13.101 12.6282L33.7647 44.0071L31.8539 46.9088L9.27936 12.6282L13.101 12.6282Z"
                              fill="white" />
                    </svg>
                </div>
            </a>
        </div>
        <ul class="header__list hide-on-mobile-tablet">
            <li class="header__item">
                <a href="~/lol-account" class="header__link">
                    RIOT GAMES
                    <div class="header__link-add" style="transform: translate(-60%,-40%);">
                        <svg width="77" height="24" viewBox="0 0 77 24" fill="none"
                             xmlns="http://www.w3.org/2000/svg">
                            <path d="M0.25 0.25H68.6493L76.75 10.0897V23.75H9.61095L0.25 13.4037V0.25Z"
                                  stroke="#3FFFF3" stroke-width="1" />
                        </svg>
                    </div>
                    <svg class="header__link-more" width="9" height="7" viewBox="0 0 9 7" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <path d="M7.53109 0.749999L4.5 6L1.46891 0.75L7.53109 0.749999Z" stroke="white" />
                    </svg>
                </a>
            </li>
            <li class="header__item" style="margin-left: 26px;">
                <a href="Product" class="header__link">
                    STEAM
                    <div class="header__link-add" style="transform: translate(-60%,-30%);">
                        <svg width="65" height="52" viewBox="0 0 65 52" fill="none"
                             xmlns="http://www.w3.org/2000/svg">
                            <path d="M6.93598 33.9488C4.32542 33.6364 3.59972 30.19 5.86262 28.8514L52.291 1.38695C53.9114 0.428399 55.9942 1.3449 56.3821 3.18719L63.515 37.0613C63.9029 38.9036 62.3666 40.5822 60.4972 40.3585L6.93598 33.9488Z"
                                  stroke="#3FFFF3" stroke-width="1" />
                        </svg>
                    </div>
                    <svg class="header__link-more" width="9" height="7" viewBox="0 0 9 7" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <path d="M7.53109 0.749999L4.5 6L1.46891 0.75L7.53109 0.749999Z" stroke="white" />
                    </svg>
                </a>
            </li>
            <li class="header__item">
                <a href="Product" class="header__link">
                    EPIC GAMES
                    <div class="header__link-add" style="transform: translate(-60%,-30%);">
                        <svg width="59" height="42" viewBox="0 0 59 42" fill="none"
                             xmlns="http://www.w3.org/2000/svg">
                            <path d="M8.74914 28.9792L8.71285 28.5437L8.35583 28.7958L0.25 34.5175V18V3.22465L27.9973 0.251722L58.75 4.21981V19.5V34.6074L51.6057 31.2735L51.4218 31.1876L51.3 31.35L43.9908 41.0956L30.193 24.3411L30.0207 24.1319L29.8261 24.3204L14.0028 39.6492L9.24153 34.888L8.74914 28.9792Z"
                                  stroke="#3FFFF3" stroke-width="1" />
                        </svg>
                    </div>
                    <svg class="header__link-more" width="9" height="7" viewBox="0 0 9 7" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <path d="M7.53109 0.749999L4.5 6L1.46891 0.75L7.53109 0.749999Z" stroke="white" />
                    </svg>
                </a>
            </li>
            <li class="header__item">
                <a href="./product.html" class="header__link">
                    FIFA
                    <svg class="header__link-more" width="9" height="7" viewBox="0 0 9 7" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <path d="M7.53109 0.749999L4.5 6L1.46891 0.75L7.53109 0.749999Z" stroke="white" />
                    </svg>
                </a>
            </li>
            <li class="header__item">
                <a href="" class="header__link hide-on-low-pc">
                    ...
                    <svg class="header__link-more" width="9" height="7" viewBox="0 0 9 7" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <path d="M7.53109 0.749999L4.5 6L1.46891 0.75L7.53109 0.749999Z" stroke="white" />
                    </svg>
                </a>
            </li>
            <li class="header__item">
                <a href="./riot.html" class="header__link">
                    ORTHER
                    <svg class="header__link-more" width="9" height="7" viewBox="0 0 9 7" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <path d="M7.53109 0.749999L4.5 6L1.46891 0.75L7.53109 0.749999Z" stroke="white" />
                    </svg>
                </a>
            </li>
            <li class="header__item"><a href="#contact" class="header__link hide-on-low-pc">CONTACT</a></li>
        </ul>
        <ul class="header__list">
            <li class="header__item" style="transform: translateX(26px);">
                <a href="~/login" class="header__link">
                    <svg class="account__icon" width="37" height="37" viewBox="0 0 37 37" fill="none"
                         xmlns="http://www.w3.org/2000/svg">
                        <circle cx="18.5" cy="18.5" r="17.5" stroke="white" stroke-width="2" />
                        <path d="M1.08828 20.3139L11.9706 18.1373C11.9706 18.1373 10.8824 9.7941 18.5 9.79411C26.1177 9.79411 25.0294 18.1373 25.0294 18.1373L35.9118 20.3139"
                              stroke="white" stroke-width="2" />
                        <circle cx="18.5" cy="15.5" r="3.5" stroke="white" stroke-width="2" />
                    </svg>
                </a>
            </li>
            <li class="header__item"><a href="~/login" class="header__link">LOGIN</a></li>
            <li class="header__item btn__menu">
                <label for="rdo__menu" style="cursor: pointer;">
                    <i class="fas fa-bars"></i>
                </label>
            </li>
        </ul>
    </header>
    <nav class="mobile__nav">
        <div class="header__logo">
            <a href="" class="header__logo-link">
                <img class="logo" src="./assets/img/logo.png" alt="">
            </a>
        </div>
        <div class="">
            <svg width="74" height="56" viewBox="0 0 74 56" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path fill-rule="evenodd" clip-rule="evenodd"
                      d="M41.6623 56L44.6556 51.4544L70.2235 12.6282L73.5161 7.62817L67.5294 7.62817L63.7364 7.62817L65.4671 4.99999L68.7597 -1.4843e-05L62.773 -1.43196e-05L11.0387 -9.79684e-06L5.05195 -9.27346e-06L8.34456 4.99999L10.0753 7.62818L9.80839 7.62818L5.98675 7.62818L-4.2288e-06 7.62818L3.29261 12.6282L28.8605 51.4544L31.8539 56L34.8473 51.4544L36.7581 48.5527L38.6689 51.4544L41.6623 56ZM36.8915 39.2588L19.3546 12.6282L54.4284 12.6282L36.8915 39.2588ZM57.7496 7.62818L57.721 7.62818L16.062 7.62818L14.3313 4.99999L59.4803 4.99999L57.7496 7.62818ZM39.7658 44.0288L41.6623 46.9088L64.2368 12.6282L60.4438 12.6282L39.8992 43.8262L39.7658 44.0288ZM13.101 12.6282L33.7647 44.0071L31.8539 46.9088L9.27936 12.6282L13.101 12.6282Z"
                      fill="white" />
            </svg>
        </div>
        <ul class="mobile__nav-list">
            <li class="mobile__nav-item"><a href="" class="mobile__nav-link">RIOT GAMES</a></li>
            <li class="mobile__nav-item"><a href="" class="mobile__nav-link">STEAM</a></li>
            <li class="mobile__nav-item"><a href="" class="mobile__nav-link">EPIC GAMES</a></li>
            <li class="mobile__nav-item"><a href="" class="mobile__nav-link">FIFA</a></li>
            <li class="mobile__nav-item"><a href="" class="mobile__nav-link">ORTHER</a></li>
            <li class="mobile__nav-item"><a href="" class="mobile__nav-link">CONTACT</a></li>
        </ul>
        <label for="rdo__menu" class="close__menu"><i class="fas fa-times"></i></label>
    </nav>
    @RenderBody()
    <!-- end header -->

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

</body>
</html>
